{
  "root": "libs/waweb/api/stackbit",
  "sourceRoot": "libs/waweb/api/stackbit/src",
  "targets": {
    "build": {
      "executor": "@nrwl/js:tsc",
      "outputs": [
        "{options.outputPath}"
      ],
      "options": {
        "outputPath": "dist/libs/waweb/api/stackbit",
        "main": "libs/waweb/api/stackbit/src/index.ts",
        "tsConfig": "libs/waweb/api/stackbit/tsconfig.lib.json",
        "assets": [
          "libs/waweb/api/stackbit/*.md"
        ]
      }
    },
    "lint": {
      "executor": "@nrwl/linter:eslint",
      "outputs": [
        "{options.outputFile}"
      ],
      "options": {
        "lintFilePatterns": [
          "libs/waweb/api/stackbit/**/*.ts"
        ]
      }
    },
    "test": {
      "executor": "@nrwl/jest:jest",
      "outputs": [
        "coverage/libs/waweb/api/stackbit"
      ],
      "options": {
        "jestConfig": "libs/waweb/api/stackbit/jest.config.js",
        "passWithNoTests": true
      }
    }
  },
  "tags": []
}
