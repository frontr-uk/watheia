{
  "root": "libs/base-ui/blog",
  "sourceRoot": "libs/base-ui/blog/src",
  "projectType": "library",
  "tags": [],
  "targets": {
    "build": {
      "executor": "@nrwl/web:rollup",
      "outputs": ["{options.outputPath}"],
      "options": {
        "outputPath": "dist/libs/base-ui/blog",
        "tsConfig": "libs/base-ui/blog/tsconfig.lib.json",
        "project": "libs/base-ui/blog/package.json",
        "entryFile": "libs/base-ui/blog/src/index.ts",
        "external": ["react/jsx-runtime"],
        "rollupConfig": "@nrwl/react/plugins/bundle-rollup",
        "compiler": "babel",
        "assets": [
          {
            "glob": "libs/base-ui/blog/README.md",
            "input": ".",
            "output": "."
          }
        ]
      }
    },
    "lint": {
      "executor": "@nrwl/linter:eslint",
      "outputs": ["{options.outputFile}"],
      "options": {
        "lintFilePatterns": ["libs/base-ui/blog/**/*.{ts,tsx,js,jsx}"]
      }
    },
    "test": {
      "executor": "@nrwl/jest:jest",
      "outputs": ["coverage/libs/base-ui/blog"],
      "options": {
        "jestConfig": "libs/base-ui/blog/jest.config.ts",
        "passWithNoTests": true
      }
    }
  }
}
